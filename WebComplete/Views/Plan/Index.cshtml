@model List<WebComplete.Models.Plan>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Planos</h2>
@if (Model.Count > 0)
    {
        <ul>
            @foreach (var item in Model)
            {
                <li>@item.Name - @item.Value Euros</li>
            }
     </ul>
    }else
    {
        <p>Não existem valores na base de dados</p>
    }